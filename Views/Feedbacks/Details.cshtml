@model multiple_choice.Models.Feedback

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Feedback</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.InterviewDesc)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Question)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Question.Question_Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Option)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Option.OptionId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Feedback_Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
