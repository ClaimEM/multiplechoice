@model IEnumerable<multiple_choice.Models.Feedback>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Question)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Option)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.User.InterviewDesc)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Question.Question_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Option.OptionId)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Feedback_Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Feedback_Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Feedback_Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
